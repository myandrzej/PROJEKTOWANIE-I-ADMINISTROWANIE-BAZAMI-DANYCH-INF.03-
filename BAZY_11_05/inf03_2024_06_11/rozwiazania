Zadanie 1.
SELECT imie, miot FROM swinki WHERE data_ur LIKE '%-07-%';

Zadanie 2.
SELECT DISTINCT swinki.data_ur, swinki.miot, rasy.rasa FROM swinki JOIN rasy ON swinki.rasy_id = rasy.id WHERE swinki.rasy_id = 1;

Zadanie 3.
SELECT imie, cena, opis FROM swinki WHERE rasy_id = 1;

Zadanie 4.
SELECT rasa FROM rasy;
